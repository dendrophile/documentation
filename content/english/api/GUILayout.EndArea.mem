BEGIN EX

	function OnGUI () {
		GUILayout.BeginArea (Rect (10,10,100,100));
		GUILayout.Button ("Click me");
		GUILayout.Button ("Or me");
		// Ends the area started above
		GUILayout.EndArea ();
	}
