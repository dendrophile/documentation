BEGIN EX

	function OnGUI () {	
		GUILayout.BeginHorizontal ("box");

		GUILayout.Button ("I'm the first button");
		GUILayout.Button ("I'm to the right");
		
		// End the horizontal group we began above
		GUILayout.EndHorizontal ();
	}
